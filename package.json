{
  "name": "konlatt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview --host",
    "build": "tsc -b && vite build",
    "build:as:debug": "asc src/wasm/as/main.ts --target debug --runtime minimal --exportRuntime",
    "build:as:release": "asc src/wasm/as/main.ts --target release --runtime minimal --exportRuntime",
    "build:emscripten-wins": "docker run --rm -it -v \"%cd%:/usr/src\" -w /usr/src emscripten/emsdk bash -c \"chmod +x ./emscripten.build.sh && ./emscripten.build.sh\"",
    "build:emscripten-unix": "docker run --rm -it -v \"${PWD}:/usr/src\" -w /usr/src emscripten/emsdk:4.0.6 bash -c \"chmod +x ./emscripten.build.sh && ./emscripten.build.sh\"",
    "build:emscripten-arm-unix": "docker run --rm -it -v \"${PWD}:/usr/src\" -w /usr/src emscripten/emsdk:4.0.6-arm64 bash -c \"chmod +x ./emscripten.build.sh && ./emscripten.build.sh\"",
    "test": "vitest run"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3-quadtree": "^3.0.1",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/d3-quadtree": "^3.0.6",
    "@types/d3-transition": "^3.0.9",
    "@types/d3-zoom": "^3.0.8",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "assemblyscript": "^0.27.32",
    "babel-plugin-react-compiler": "^19.0.0-beta-27714ef-20250124",
    "eslint": "^9.17.0",
    "eslint-plugin-react-compiler": "^19.0.0-beta-21e868a-20250216",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.6",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.1"
  }
}
