{
  "name": "konlatt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview --host",
    "build": "tsc -b && vite build",
    "build:as": "cd benchmarks/as && npm run build",
    "build:emscripten-wins": "docker run --rm -it -v \"%cd%:/usr/src\" -w /usr/src emscripten/emsdk bash -c \"chmod +x ./emscripten.build.sh && ./emscripten.build.sh\"",
    "build:emscripten-unix": "docker run --rm -it -v \"${PWD}:/usr/src\" -w /usr/src emscripten/emsdk:4.0.6 bash -c \"chmod +x ./emscripten.build.sh && ./emscripten.build.sh\"",
    "build:emscripten-arm-unix": "docker run --rm -it -v \"${PWD}:/usr/src\" -w /usr/src emscripten/emsdk:4.0.6-arm64 bash -c \"chmod +x ./emscripten.build.sh && ./emscripten.build.sh\"",
    "test": "vitest run"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3-quadtree": "^3.0.1",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/d3-quadtree": "^3.0.6",
    "@types/d3-transition": "^3.0.9",
    "@types/d3-zoom": "^3.0.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.2.3"
  }
}